aws_region = "us-east-1"
project = "th-insights"
env = "staging"
create_vpc = true
# If create_vpc = false, set these values:
# vpc_id = "vpc-..."
# public_subnet_ids = ["subnet-...", "subnet-..."]
# private_subnet_ids = ["subnet-...", "subnet-..."]

cloudfront_cert_arn = "arn:aws:acm:us-east-1:<account>:certificate/<id>" # set if you want CF with custom domain
# domain_name and hosted_zone_id optional for DNS
# domain_name = "example.com"
# hosted_zone_id = "Z..."

# Optional JWT secret (or store later in Secrets Manager)
jwt_secret = "change-me"

# GitHub OIDC repo (owner/repo). Edit to match your repo.
# github_oidc_repo = "owner/repo"
